@page "/ordersummarys/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3>View</h3>

<FormComponent ordersummary="ordersummary" Disabled="true" />

@code {
    [Parameter] public int id { get; set; }
    OrderSummary ordersummary = new OrderSummary();

    protected async override Task OnParametersSetAsync()
    {
       ordersummary = await
       _client.GetFromJsonAsync<OrderSummary>($"{Endpoints.OrderSummarysEndpoint}/{id}");
    }
}